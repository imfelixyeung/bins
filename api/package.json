    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.3",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "postgres": "^3.4.3",
    "tsx": "^4.1.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.14.201",
    "@types/node-cron": "^3.0.11",
    "drizzle-kit": "^0.20.2",
    "typescript": "^5.0.0"
